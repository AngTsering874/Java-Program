class Person {
    private String name; 
    public void setName(String Name) {
        name = Name;
    }


    public String getName() {
        return name;
    }
}

public class Main {
    public static void main(String[] args) {
        Person p = new Person();
        p.setName("Ang Tsering");  
        System.out.println("Name: " + p.getName()); 
    }
}
